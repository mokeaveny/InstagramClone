services:
  instagramclone.api:
    build:
      context: .
      dockerfile: InstagramClone.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: instagramcloneapi
  sql_in_dc:
    build:
      context: .
      dockerfile: sql/sql.Dockerfile
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: abcDEF123#
    ports:
    - mode: ingress
      target: 1433
      published: 1440
      protocol: tcp
    restart: always