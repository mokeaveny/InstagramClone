services:
  instagramclone.api:
    build:
      context: C:\Users\micha\Documents\Microservices\Docker Projects\InstagramClone
      dockerfile: InstagramClone.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: InstagramClone.Api
      target: base
    container_name: InstagramClone.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: instagramcloneapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/InstagramClone.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    tty: true
    volumes:
    - C:\Users\micha\Documents\Microservices\Docker Projects\InstagramClone\InstagramClone.Api:/app:rw
    - C:\Users\micha\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\micha\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\micha\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\micha\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\micha\Documents\Microservices\Docker Projects\InstagramClone:/src:rw
  sql_in_dc:
    build:
      context: C:\Users\micha\Documents\Microservices\Docker Projects\InstagramClone
      dockerfile: sql/sql.Dockerfile
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: abcDEF123#
    ports:
    - published: 1440
      target: 1433
    restart: always
version: '3.4'